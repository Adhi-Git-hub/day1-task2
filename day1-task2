n = 5
for i in range(1,n+1):
    if (i % 2)==0:
        for k in range(1,i+1):
            print("#",end='')
    else:
        for k in range(1,i+1):
            print("*",end='')
    print()
for i in range(n,1,-1):
    if (i % 2)==0:
        for k in range(i,1,-1):
            print("*",end='')
    else:
        for k in range(i,1,-1):
            print("#",end='')
    print()
